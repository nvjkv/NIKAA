      "cell_type": "code",
      "source": [
        "# Load mtcars data set\n",
        "mtcars = pd.read_csv(\"mtcars.csv\")\n",
        "\n",
        "st = 1/(mtcars['hp']/mtcars['wt'])*440\n",
        "mtcars['st'] = st\n",
        "\n",
        "mtcars.plot(kind=\"scatter\",\n",
        "           x=\"st\",\n",
        "           y=\"qsec\",\n",
        "           figsize=(9,9),\n",
        "           color=\"black\");"
      ],
      "metadata": {
        "id": "SxAs-GaGYCc8",
        "colab": {
          "base_uri": "https://localhost:8080/",
